             x:Class="Kwaliteit.Pages.SettingsPage">
    <ContentPage.Content>
        <StackLayout>
            <Button Text="Maak Beuk" Command="{Binding OpenMaakBeukCommand}" HorizontalOptions="Center" Style="{StaticResource PrimaryButton}"></Button>
